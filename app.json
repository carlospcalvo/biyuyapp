{
  "expo": {
    "name": "biyuyapp",
    "slug": "biyuyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/biyuyapp_icon.png",
    "splash": {
      "image": "./assets/biyuyapp_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#12263A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
